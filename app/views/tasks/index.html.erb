<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<ul>
<% @task.each do |task| %>
  <li>title:<%= task.title %></li>
  <li>content:<%= task.content %></li>
  <%= link_to "edit", edit_task_path(task.id) %>
  <%= link_to "del", task_path(task.id), method: :delete %>
  <%= link_to "show", task_path(task.id) %>
<% end %>
</ul>
